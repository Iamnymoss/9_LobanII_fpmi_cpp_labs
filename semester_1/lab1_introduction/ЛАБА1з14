#include <iostream>
using namespace std;

int main()
{
    int n;
    cin >> n;

    if (n < 0) {
        cout << "otricatelnoe" << endl;
        return 1;
    }

    int summa = 0;
    int temp = n;

    while (temp > 0) {
        summa += temp % 10;
        temp /= 10;
    }

    cout << summa << endl;

    return 0;
}
